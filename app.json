{
  "expo": {
    "name": "hqcs2",
    "slug": "hqcs2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "hqcs",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.killerchip.hqcs2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "net.killerchip.hqcs2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/antiqua400.ttf",
            "assets/fonts/caesar400.ttf",
            "node_modules/@expo-google-fonts/caudex/Caudex_400Regular.ttf",
            "node_modules/@expo-google-fonts/caudex/Caudex_400Regular_Italic.ttf",
            "node_modules/@expo-google-fonts/caudex/Caudex_700Bold.ttf",
            "node_modules/@expo-google-fonts/caudex/Caudex_700Bold_Italic.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8ecb178e-799c-4cc3-bea8-3566eba95b4f"
      }
    },
    "owner": "killerchip"
  }
}
